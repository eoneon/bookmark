<div class="row">
  <div class="col-md-8">
    <div class="media">
      <div class="media-body">
        <h2 class="media-heading"><%= @user.email %></h2>
        <% @user_liked_bookmarks.each do |bookmark| %>
          <li><%= bookmark.url %></li>
          <%= render partial: 'likes/like', locals: { bookmark: bookmark } %>
        <% end %>
      </div>
    </div>
  </div>
</div>
